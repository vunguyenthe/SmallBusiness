        <column name="id" valueComputed="0"/>
        <column name="journalType" valueComputed="'Note'"/>
    </insert>
    <insert tableName="journalType">
        <column name="id" valueComputed="1"/>
        <column name="journalType" valueComputed="'Email'"/>
    </insert>
    <insert tableName="journalType">
        <column name="id" valueComputed="2"/>
        <column name="journalType" valueComputed="'Appointment'"/>
    </insert>
    <insert tableName="journalType">
        <column name="id" valueComputed="3"/>
        <column name="journalType" valueComputed="'Task'"/>
    </insert>
    <insert tableName="journalType">
        <column name="id" valueComputed="4"/>
        <column name="journalType" valueComputed="'Interview'"/>
    </insert>
    <insert tableName="journalType">
        <column name="id" valueComputed="5"/>
        <column name="journalType" valueComputed="'Meeting'"/>
    </insert>
</changeSet>
