<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd">
  <changeSet id="27" author="hr4">
   <preConditions onFail="MARK_RAN">
        <not>
            <tableExists tableName="peoplecvcontent"/>
        </not>
    </preConditions>
    <createTable tableName="peoplecvcontent">
      <column name="id" type="int" autoIncrement = "true"><constraints primaryKey="true" nullable="false"/></column> 
      <column name="peopleId" type="int"/>
      <column name="content" type="TEXT"/>
    </createTable>   
  </changeSet>
</databaseChangeLog>
